{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/assets/*", "/*.ico", "/*.png", "/*.svg"]
  },
  "platform": {
    "apiRuntime": "node:20"
  },
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/aad?prompt=select_account",
      "statusCode": 302
    }
  }
}
