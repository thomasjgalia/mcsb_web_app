# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Authentication Settings
# Set to 'true' to bypass Supabase authentication for local development
VITE_DISABLE_AUTH=false

# Oracle Database Configuration (Backend only - DO NOT prefix with VITE_)
ORACLE_USER=ADMIN
ORACLE_PASSWORD=your-oracle-password
ORACLE_CONNECTION_STRING=your-connection-string
# Oracle Wallet path (for local development)
ORACLE_WALLET_LOCATION=/path/to/wallet
ORACLE_WALLET_PASSWORD=your-wallet-password

# Application Settings
NODE_ENV=development

# OpenAI Configuration (Backend only - DO NOT prefix with VITE_)
OPENAI_API_KEY=your-openai-api-key-here

# UMLS Configuration (Backend only - DO NOT prefix with VITE_)
# Get your API key from: https://uts.nlm.nih.gov/uts/signup-login
UMLS_API_KEY=your-umls-api-key-here

# Azure SQL Database Configuration (for user data storage)
AZURE_SQL_CONNECTION_STRING=Server=your-server.database.windows.net;Database=your-database;User Id=your-user;Password=your-password;Encrypt=true;TrustServerCertificate=true;
